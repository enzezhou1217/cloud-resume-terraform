{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "PublicReadGetObject",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::cloud-resume-bucket-enzezhou/*"
        },
        {
            "Sid": "S3SyncforGitHub",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "arn:aws:iam::023639994151:user/enzo",
                    "arn:aws:iam::023639994151:user/UserForGitHubActionS3Sync"
                ]
            },
            "Action": "*",
            "Resource": "arn:aws:s3:::cloud-resume-bucket-enzezhou/*"
        }
    ]
}